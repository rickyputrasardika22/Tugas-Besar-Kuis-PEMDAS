package tugasBesar;
import java.util.Scanner;
public class RickyPutraSardika_07 
{
	public static void main(String[] args) 
	{
		Scanner ricky = new Scanner(System.in);
		int angka_ricky;
		
		System.out.print("Masukkan Angka = ");
		angka_ricky = ricky.nextInt();
		
		int [] kebalikanAngka_ricky = new int [angka_ricky];
		
		System.out.print("Kebalikan dari Angka " + angka_ricky + " adalah " + kebalikanAngka_ricky);
	}
}
