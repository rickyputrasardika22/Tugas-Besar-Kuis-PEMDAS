package tugasBesar;

public class RickyPutraSardika_13 
{
	public static void main(String[] args) 
	{
		int k_ricky = 0;
		for (int i_ricky = 5; i_ricky > 0; i_ricky--)
		{
			for (int j_ricky = 5; j_ricky > 0; j_ricky--)
			{
				if(i_ricky == 1 || i_ricky == 5)
				{
					if(j_ricky == 1 || j_ricky == 5)
					{
						k_ricky--;
					}	
				}	
			}
		System.out.println("");	
		}	
	}
}
