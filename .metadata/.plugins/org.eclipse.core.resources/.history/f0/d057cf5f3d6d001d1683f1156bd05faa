package tugasBesar;
import java.util.Scanner;
public class RickyPutraSardika_06 
{
	public static void main(String[] args) 
	{
		int k = 0;
		for (int i = 1; i<=5; i++)
		{
			for (int j = 1; j<=5; j++)
			{
				if (i == 1 || i == j)
				{
					if (j > 5)
					{
						System.out.print(j);;
					}
					else if (j < 5)
					{
						k--;
						System.out.print(k);
					}
				}	
			}
		System.out.println("");
		}	
	}
}
