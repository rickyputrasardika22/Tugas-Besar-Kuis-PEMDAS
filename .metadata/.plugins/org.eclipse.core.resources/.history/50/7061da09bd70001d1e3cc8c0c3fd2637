package tugasBesar;
import java.util.Scanner;
public class RickyPutraSardika_23 
{
	public static void main(String[] args) 
	{
		Scanner ricky = new Scanner(System.in);
		char karakter_ricky;
		
		System.out.print("Masukkan Karakter " );
		karakter_ricky = ricky.next().charAt(0);
		
		
		char [] alfabet_ricky = new char[karakter_ricky];
		char [] angka_ricky = new char[karakter_ricky];
		
		for(alfabet_ricky = 'A'; alfabet_ricky <= 'Z'; ++alfabet_ricky)
		{
			
			if(karakter_ricky == alfabet_ricky)
			{
				System.out.print("Karakter " + karakter_ricky + " adalah Alfabet");
			}
			else if(karakter_ricky == alfabet_ricky)
			{
				System.out.print("Karakter " + karakter_ricky + " adalah Alfabet");
			}	
		}	
//		char c;
//
//	    for(c = 'A'; c <= 'Z'; ++c)
//	      System.out.print(c + " ");
	}
}
